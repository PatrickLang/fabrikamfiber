<!DOCTYPE html> 
<html lang="en"> 
<head>
    <meta charset="utf-8" />
    
    <link rel="shortcut icon" href="@Url.Content("~/Content/images/favicon.ico")" /> 	

    <title>@ViewBag.Title</title>
    <meta http-equiv="X-UA-Compatible" content="IE=9" />
    <meta http-equiv="X-XSS-Protection" content="0" />
    <link href="@Url.Content("~/Content/style.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/themes/base/jquery.ui.all.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jQuery.tmpl.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/knockout-1.2.0.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/knockout.mapping-latest.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-ui-1.8.11.js")" type="text/javascript"></script>

</head>
<body>
    <input type="hidden" name="__VIEWSTATE" id="__VIEWSTATE" value="" />

        <header>
        
            
         
            <hgroup> 
              <h1><img src="@Url.Content("~/Content/images/logo_text.png")" alt="Fabrikam Fiber" /></h1> 
              <h2>Support</h2> 
            </hgroup> 

            <nav>
                <ul>
                    <li>@Html.ActionLink("Dashboard", "Index", "Home")</li>
                    <li>@Html.ActionLink("Tickets", "Index", "ServiceTickets")</li>
                    <li>@Html.ActionLink("Customers", "Index", "Customers")</li>
                    <li>@Html.ActionLink("Employees", "Index", "Employees")</li>
                    <li>@Html.ActionLink("Reports", "Index", "Reports")</li>
                </ul>
            </nav>

        </header>

        
        
    <div id="container">
    
            <aside id="profile">

                @if (IsSectionDefined("Left"))
                {
                    RenderSection("Left");
                }
                else
                { 
                    @RenderPage("_Profile.cshtml")  
                }
             
            </aside>
        
            <section id="content">
                
           @RenderBody()
        </section>
      


    <!-- end of container -->
    </div>
    
    <p id="copyright">
        &copy; 2013 Fabrikam Fiber
    </p>
</body>
</html>
